<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:ns1="http://nursery.orange.com/appliance/V1" targetNamespace="http://nursery.orange.com/appliance/V1" elementFormDefault="qualified" attributeFormDefault="unqualified">

