<Location  /ApplianceManagerAdmin/auth/>

	Include /usr/local/nursery/ApplianceManager/RunTimeAppliance/apache/conf/vhAppliance/nursery-appliance-settings.inc	
	OSAEnable Off
	
	
	
	OSAResourceName ApplianceManagerAdminAuth

	OSACheckGlobalQuotas Off
	OSACheckUserQuotas Off


	OSALogHit Off
	
	ProxyPassReverseCookieDomain 127.0.0.1 r-lnx-jmjb0521.rd.francetelecom.fr




	ProxyPassReverse http://127.0.0.1:82/ApplianceManager/auth/
		
</Location>

<Location  /ApplianceManagerAdmin/auth/token>

	Include /usr/local/nursery/ApplianceManager/RunTimeAppliance/apache/conf/vhAppliance/nursery-appliance-settings.inc	
	AuthBasicAuthoritative Off
	OSAAuthoritative On
	OSAEnable on
	OSABasicAuthEnable On
	OSACookieAuthEnable On
	OSACookieAuthName OSAAuthToken
	OSACookieAuthTTL 60
	OSARequire valid-user
	OSAIdentityHeadersMapping userName,X_REMOTE_USER;firstName,NURS_FIRSTNAME;lastName,NURS_LASTNAME;entity,NURS_ENTITY;emailAddress,NURS_EMAIL
	
	
	
	OSAResourceName ApplianceManagerAdminAuthToken

	OSACheckGlobalQuotas Off
	OSACheckUserQuotas Off


	OSALogHit Off
	
	ProxyPassReverseCookieDomain 127.0.0.1 r-lnx-jmjb0521.rd.francetelecom.fr




	ProxyPassReverse http://127.0.0.1:82/ApplianceManager/auth/token
		
</Location>

<Location  /path/to/app>

	Include /usr/local/nursery/ApplianceManager/RunTimeAppliance/apache/conf/vhAppliance/nursery-appliance-settings.inc	
	OSAEnable Off
	
	
	
	OSAResourceName TestWS

	OSACheckGlobalQuotas Off
	OSACheckUserQuotas Off


	OSALogHit Off
	
	ProxyPassReverseCookieDomain .websocket.org r-lnx-jmjb0521.rd.francetelecom.fr
	ProxyPassReverseCookiePath /  /path/to/app




